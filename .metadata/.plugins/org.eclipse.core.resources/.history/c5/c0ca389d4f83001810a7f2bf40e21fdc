package com.spring.miracom;

public class Printer {
	public Printer(String name, String brand) {
		this.name = name;
		this.brand = brand;
	}
	
	
	public void setPrinter(String printer) {
		this.printer = printer;
	}
	public String getInfo() {
		return "[" + name + "]" + "헬로";		
	}

}
