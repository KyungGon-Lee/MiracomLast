package com.spring.miracom;
import java.util.*;

public class Car {
	String name;
	String color;
	Date date;	
	public Car(String name, String color) {
		this.name = name;
		this.color = color;
		this.date = new Date();
	}
	public void setColor(String color) {
		this.color = color;
	}
	public String getInfo() {
		return name + "," + color + "," date;
	}
	

}
