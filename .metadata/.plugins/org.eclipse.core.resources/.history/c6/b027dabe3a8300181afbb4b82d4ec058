package com.spring.miracom;

public class CarService {
	private Car car;
	
	public CarService(){
	}// 이게 정의되어있어야
	
	public CarService(Car car){
		this.car = car;
	}
	public void park() {
		System.out.println("=========start==========");
		System.out.println(car.getInfo());
		System.out.println("==========end=========");
		
	}
	public void setCar(Car car) {
		this.car = car;
	}

}
